{
  "name": "ahamo-dummy-demo2-frontend-workspace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:api": "jest --testPathPattern=integration/api",
    "test:hooks": "jest --testPathPattern=integration/hooks",
    "test:pages": "jest --testPathPattern=integration/pages",
    "test:e2e": "playwright test",
    "test:a11y": "playwright test --grep @a11y",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tokens:build": "style-dictionary build",
    "tokens:watch": "style-dictionary build --watch",
    "tokens:clean": "rm -rf build/ src/styles/tokens/",
    "depcheck": "madge --circular --extensions ts,tsx src/"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@mswjs/interceptors": "^0.39.3",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@storybook/addon-designs": "^10.0.2",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.81.5",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.74",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-onboarding": "^9.0.16",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/nextjs-vite": "^9.0.16",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@tokens-studio/sd-transforms": "^2.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-storybook": "^9.0.16",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "madge": "^8.0.0",
    "msw": "^2.0.11",
    "node-fetch": "^3.3.2",
    "playwright": "^1.53.2",
    "prettier": "^3.1.1",
    "storybook": "^9.0.16",
    "style-dictionary": "^5.0.1",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
